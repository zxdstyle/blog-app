{
  "name": "peachicksblog",
  "version": "0.1.0",
  "description": "Peachick's blog、Peachick的博客、博客系统",
  "main": "index.js",
  "scripts": {
    "clear": "node ./build/webpack.clear.js",
    "webpack-dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.dev.js",
    "webpack-build": "cross-env NODE_ENV=production webpack --config ./build/webpack.prod.js",
    "server-dev": "cross-env NODE_ENV=development nodemon ./server/server.js",
    "server": "nodemon ./server/server.js",
    "dev": "concurrently -n Clear-Serve,Webpack-Serve,Server-Serve \"npm run clear\" \"npm run webpack-dev\" \"npm run server-dev\"",
    "build": "concurrently -n Clear-Serve,Webpack-Serve \"npm run clear\" \"npm run webpack-build\"",
    "deploy": "rm -rf server/public/* && cp -r client/dist/** server/public",
    "view": "cross-env NODE_ENV=prudction npm run server",
    "lint": "eslint --ext .js ./"
  },
  "nodemonConfig": {
    "verbose": false,
    "ignore": [
      "client/*",
      "build/*",
      "node_modules/*",
      "server/logs"
    ],
    "delay": "1500"
  },
  "repository": "https://github.com/Peachick/blog-app.git",
  "author": "Peachick <wsm_1105@163.com>",
  "license": "MIT",
  "private": false,
